{
    "alias": "box.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": ["src/lib"],
    "extract": true,
    "files": [
        "LICENSE",
        "res/schema.json",
        "src/vendors/herrera-io/phar-update/res/schema.json"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "File",
                "mikey179",
                "Net",
                "phpunit",
                "phpunit-test-case",
                "Tester",
                "Tests",
                "tests",
                "yaml"
            ],
            "in": "src/vendors"
        }
    ],
    "git-commit": "git-commit",
    "git-version": "git-version",
    "main": "bin/box",
    "output": "box-@git-version@.phar",
    "replacements": {
        "manifest_url": "https://box-project.github.io/box2/manifest.json"
    },
    "shebang": "#!/usr/bin/env php -d phar.readonly=0",
    "stub": true
}
